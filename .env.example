HOST=127.0.0.1
PORT=3333
NODE_ENV=development
# app config
APP_NAME=AdonisJs
APP_DEBUG=true
APP_URL=http://${HOST}:${PORT}
# disable cache views
CACHE_VIEWS=false
# app key generated with command adonis generate:key
APP_KEY=
# database config
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=root
DB_PASSWORD=admin
DB_DATABASE=purek
# hashing config
HASH_DRIVER=bcrypt

# Please make different file .env.testing for Testing eg below
# NODE_ENV=testing
# DB_CONNECTION=sqlite
# DB_DATABASE=purek_test
